#include<iostream>
#include <ctime>
using namespace std;
void print_matrix(int **array, int n) {
	for (int i = 0; i <n; i++) {
		for (int j = 0; j < n; j++) {
			cout << array[i][j] << " ";
		}
		cout << endl;
	}
}
void random_matrix(int** array, int n) {
	for (int i = 0; i < n; i++) {
		for (int j = 0; j < n; j++) {
			array[i][j] = rand() % 90 + 10;
		}
	}
}
void one(int** array, int n) {
	int a; int b;
	cin >> a >> b;
	for (int i = 0; i < n; i++) {
		int tmp;
		tmp = array[a-1][i];
		array[a-1][i] = array[b-1][i];
		array[b-1][i] = tmp;
	}
}
void delete_mas(int** array, int n) {
for (int i = 0; i < n; i++) {
delete[] array[i];
}
delete[] array;
}
int main() {
	srand(time(0));
	int n;
	cin >> n;
	int** array = new int* [n];
	for (int i = 0; i < n; i++) {
		array[i] = new int[n];
	}
	random_matrix(array, n);
	print_matrix(array, n);
	one(array, n);
	print_matrix(array, n);
	delete_mas(array,n);
}
