#include <iostream>
#include <ctime>
using namespace std;
void print_m(int** array, int n) {
	for (int i = 0; i < n; i++) {
		for (int j = 0; j < n; j++) {
			cout << array[i][j] << " ";
		}
		cout << endl;
	}
}
void random_m(int** array, int n) {
	for (int i = 0; i < n; i++) {
		for (int j = 0; j < n; j++) {
			cin >> array[i][j];
		}
	}
}
void one(int** array, int n) {
	int c; c = 0; int x; cin >> x;
	for (int i = 0; i < n; i++) {
		for (int j = 0; j < n; j++) {
			if (array[i][j]== x) {
				c++;
			}
		}
	}
	cout << c;
}

int main() {
	srand(time(0));
	int n;
	cin >> n;
	int** array = new int* [n];
	for (int i = 0; i < n; i++) {
		array[i] = new int[n];
	};
	random_m(array, n);
	//print_m(array, n);
	one(array, n);
	//print_m(array, n);
	delete[] array;
}
