#include<iostream>
#include <ctime>
using namespace std;
void print_matrix(int** array, int n) {
	for (int i = 0; i < n; i++) {
		for (int j = 0; j < n; j++) {
			cout << array[i][j] << " ";
		}
		cout << endl;
	}
}
void random_matrix(int** array, int n) {
	for (int i = 0; i < n; i++) {
		for (int j = 0; j < n; j++) {
			array[i][j] = rand() % 90 + 10;
		}
	}
}
void three(int** array, int n) {
	int sum;
	for (int i = 0; i < n; i++) {
		sum = 0;
		for (int j = 0; j < n; j++) {
			sum = sum + array[j][i];
		}
		cout << sum << endl;
	}
}
void delete_mas(int** array, int n) {
for (int i = 0; i < n; i++) {
delete[] array[i];
}
delete[] array;
}
int main() {
	srand(time(0));
	int n;
	cin >> n;
	int** array = new int* [n];
	for (int i = 0; i < n; i++) {
		array[i] = new int[n];
	}
	random_matrix(array, n);
	print_matrix(array, n);
	three(array, n);
	delete_mas(array,n);
}
